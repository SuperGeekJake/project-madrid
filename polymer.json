{
  "lint": {
    "rules": [ "polymer-2" ]
  },
  "builds": [
    {
      "name": "es5",
      "js": { "minify": true, "compile": true },
      "css": { "minify": true },
      "html": { "minify": true },
      "addServiceWorker": false,
      "insertPrefetchLinks": false
    }
  ],
  "extraDependencies": [
    "team-player/**/*",
    "communicator/**/*",
    "go-getter/**/*",
    "innovator/**/*",
    "favicon.ico",
    "fb-quiz.png",
    "android-icon-*",
    "apple-icon-*",
    "ms-icon-*",
    "favicon-*"
  ]
}
